                const data = response.data;
                
                if (data.id_provinsi === 31) {
                    window.location.href = `https://rri.co.id/pusat-pemberitaan`;
                } else {
                    window.location.href = `https://rri.co.id/${data.kode_provinsi}`;
                }
            }
